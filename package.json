    "@types/react-dom": "^16.0.5",
    "@types/shelljs": "^0.7.8",
    "@types/supertest": "^2.0.4",
    "@types/validator": "^9.4.0",
    "@types/winston": "^2.3.9",
    "awesome-typescript-loader": "^5.0.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3",
    "shelljs": "^0.8.1",
    "source-map-loader": "^0.2.3",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.3",
    "ts-node": "^6.0.0",
    "tsc": "^1.20150623.0",
    "tslint": "^5.9.1",
    "typescript": "^2.9.0-dev.20180426"
  }
}
