{
  "name": "budgetarium",
  "author": "Danil Kazimirov",
  "license": "MIT",
  "version": "0.0.2",
  "private": true,
  "main": "./src/server/bin/www",
  "scripts": {
    "start": "npm run serve",
    "serve": "NODE_ENV=production node build/www.js",
    "build": "npm run lint && npm run build-ts && npm run copy-static-assets",
    "serve-debug": "NODE_ENV=development nodemon build/www",
    "dev": "npm run copy-static-assets && concurrently -k -p \"[{name}]\" -n \"TS,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -p ./tslint.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts"
  },
  "_moduleAliases": {
    "@config": "build/config/configuration",
    "@types": "build/types",
    "@controllers": "build/controllers",
    "@utils": "build/utils/index",
    "@routes": "build/routes/index",
    "@DB": "build/DB",
    "@GraphQL": "build/controllers/graphql"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.5",
    "bcrypt": "^2.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "busboy": "^0.2.14",
    "busboy-body-parser": "^0.3.2",
    "class-validator": "^0.8.5",
    "compression": "^1.7.2",
    "config": "^1.30.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-validator": "^5.1.2",
    "graphql": "^0.13.2",
    "graphql-tools": "^2.24.0",
    "lodash": "^4.17.5",
    "merge-graphql-schemas": "^1.5.1",
    "module-alias": "^2.0.6",
    "mongoose": "^5.0.15",
    "mongoose-paginate": "^5.0.3",
    "mongoose-schema-to-graphql": "^2.7.2",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "validator": "^9.4.1",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/compression": "0.0.36",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-graphql": "^0.6.1",
    "@types/express-validator": "^3.0.0",
    "@types/graphql": "^0.13.0",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.107",
    "@types/mongoose": "^5.0.9",
    "@types/mongoose-paginate": "^5.0.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.5",
    "@types/passport": "^0.4.5",
    "@types/passport-facebook-token": "^0.4.33",
    "@types/passport-http-bearer": "^1.0.32",
    "@types/passport-jwt": "^3.0.1",
